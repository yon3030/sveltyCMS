{
	"name": "simplecms",
	"version": "0.0.3",
	"scripts": {
		"---- DEVELOP ----------------------------------------------------------": "",
		"dev": "vite",
		"devv": "LAUNCH_EDITOR='code' vite ",
		"devvv": "paraglide-js compile --project ./project.inlang",
		"---- BUILD ----------------------------------------------------------": "",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"---- CHECK & TEST ----------------------------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && playwright test",
		"test:e2e": "pnpm dev & pnpx wait-on http://localhost:5173 && playwright test",
		"---- LINT & FORMAT ------------------------------------------------------------": "",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"---- OTHERD ----------------------------------------------------------": "",
		"vite": "vite dev --open ",
		"preview": "vite preview"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@playwright/test": "^1.40.1",
		"@semantic-release/git": "^10.0.1",
		"@skeletonlabs/skeleton": "^2.5.1",
		"@skeletonlabs/tw-plugin": "^0.2.4",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.6",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/mime-types": "^2.1.4",
		"@types/node": "^20.10.1",
		"@types/node-os-utils": "^1.3.4",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-svelte": "^2.35.1",
		"lightningcss": "^1.22.1",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"semantic-release": "^22.0.8",
		"sharp": "^0.33.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-dnd-action": "^0.9.33",
		"sveltekit-superforms": "^1.11.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0",
		"vite-plugin-tailwind-purgecss": "^0.1.4",
		"zod": "^3.22.4",
		"@inlang/paraglide-js": "1.0.0-prerelease.14"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
