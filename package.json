		"mapbox-gl": "^2.13.0",
		"moment": "^2.29.4",
		"mongoose": "7.0.1",
		"nodemailer": "6.9.1",
		"openai": "^3.2.1",
		"svelte-lexical": "0.2.0",
		"svelte-tel-input": "^1.3.2",
		"tabulator-tables": "^5.4.4",
