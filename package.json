{
	"name": "SimpleCMS",
	"version": "0.0.3",
	"scripts": {
		"dev": "npm-run-all --parallel vite typesafe-i18n",
		"devv": "LAUNCH_EDITOR='code' npm-run-all --parallel vite typesafe-i18n",
		"vite": "vite dev --open",
		"build": "vite build",
		"package": "svelte-kit package",
		"prepare": "husky install",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"typesafe-i18n": "typesafe-i18n",
		"start": "npm run dev",
		"stop": "npx kill-port 5173"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "1.0.0",
		"@sveltejs/adapter-node": "latest",
		"@sveltejs/kit": "1.11.0",
		"@types/cookie": "0.5.1",
		"@types/lodash": "^4.14.191",
		"@types/mapbox__mapbox-gl-geocoder": "^4.7.3",
		"@types/mapbox-gl": "^2.7.10",
		"@types/node": "^18.15.3",
		"@types/sharp": "^0.31.1",
		"@typescript-eslint/eslint-plugin": "5.55.0",
		"@typescript-eslint/parser": "5.55.0",
		"autoprefixer": "10.4.14",
		"eslint": "8.36.0",
		"eslint-config-prettier": "8.7.0",
		"eslint-plugin-svelte3": "4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.0",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.21",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.4",
		"prettier-plugin-svelte": "2.9.0",
		"sharp": "^0.31.3",
		"svelte": "3.56.0",
		"svelte-check": "3.1.4",
		"svelte-preprocess": "5.0.2",
		"sveltekit-superforms": "^0.5.25",
		"tailwindcss": "3.2.7",
		"tslib": "2.5.0",
		"typesafe-i18n": "5.24.2",
		"typescript": "~4.9.5",
		"vite": "4.1.4",
		"zod": "^3.21.4"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.2.4",
		"@iconify/svelte": "^3.1.0",
		"@lucia-auth/adapter-mongoose": "0.6.0",
		"@lucia-auth/oauth": "^0.7.1",
		"@lucia-auth/sveltekit": "^0.6.10",
		"@mapbox/mapbox-gl-geocoder": "^5.0.1",
		"@mapbox/mapbox-gl-language": "^1.0.1",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/line-clamp": "0.4.2",
		"@tailwindcss/typography": "0.5.9",
		"@tanstack/match-sorter-utils": "^8.7.6",
		"@tanstack/svelte-table": "^8.7.9",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/nodemailer": "6.4.7",
		"await-to-js": "^3.0.0",
		"axios": "^1.3.4",
		"cheerio": "1.0.0-rc.12",
