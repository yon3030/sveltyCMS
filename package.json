{
	"name": "simplecms",
	"description": "A powerful Headless CMS with sveltekit power",
	"version": "0.0.4",
	"author": "Rar9",
	"scripts": {
		"---- DEVELOP ----------------------------------------------------------": "",
		"dev": "vite",
		"devv": "LAUNCH_EDITOR='code' vite ",
		"inlang": "paraglide-js compile --project ./project.inlang",
		"---- BUILD ----------------------------------------------------------": "",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"---- CHECK & TEST ----------------------------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && playwright test",
		"test:e2e": "pnpm dev & pnpx wait-on http://localhost:5173 && playwright test",
		"---- LINT & FORMAT ------------------------------------------------------------": "",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"---- OTHER ----------------------------------------------------------": "",
		"vite": "vite dev --open ",
		"preview": "vite preview"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rar9/SimpleCMS"
	},
	"bugs": {
		"url": "https://github.com/Rar9/SimpleCMS/issues"
	},
	"keywords": [
		"cms",
		"content-management-system",
		"inlang",
		"i18n",
		"internationalization",
		"headless",
		"skeleton",
		"skeleton-cms",
		"svelte",
		"tailwind",
		"tailwindcss",
		"sveltekit",
		"rich-text-editor",
		"lexical-editor",
		"i18n",
		"mongodb"
	],
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
